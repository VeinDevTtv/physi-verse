{
  "name": "physi-verse",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "dev:desktop": "cross-env NODE_ENV=development concurrently \"next dev -p 3000\" \"nodemon --watch electron --ext ts --exec \"electron -r ts-node/register/transpile-only electron/main.ts\"\"",
